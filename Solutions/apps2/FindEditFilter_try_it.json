        "fields": [{
            "label": "Hint",
            "fieldName": "searchBox.hint",
            "type": "string",
            "tooltip": "Search hint (e.g. Find a Pre-Incident Plan, Find a Park)"
        }, {
            "label": "Find Layers",
            "fieldName": "featureSearcher.searchLayerName",
            "type": "string",
